// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model People {
  id        String     @id @default(uuid())
  firstName String
  lastName  String
  email     String  @unique
  birthDate DateTime 
  balanceMonth  Int 
}
